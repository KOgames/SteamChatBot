<Window x:Class="SteamChatBot.LoggerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SteamChatBot"
        mc:Ignorable="d"
        Title="Logger Window" Height="685" Width="794" Icon="scb.ico">
    <Grid>
        <ListBox x:Name="loggerBox" HorizontalAlignment="Center" Height="624" Margin="15,16,14,14" VerticalAlignment="Center" Width="757" Background="Black" Foreground="White" RenderTransformOrigin="0.5,0.5">
            <ListBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.043"/>
                    <TranslateTransform/>
                </TransformGroup>
            </ListBox.RenderTransform>
        </ListBox>
    </Grid>
</Window>
